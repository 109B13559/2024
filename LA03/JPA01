import java.util.Scanner;

public class Hello {
    public static void main(String[] args) {
        Scanner scn= new Scanner(System.in);

        System.out.print("Input: ");
        int N = scn.nextInt();

        int sum = 0;
        for (int i = 1; i <= N; i++) {
            sum += i;
        }

        System.out.println("Sum of 1 to " + N + " is: " + sum);
    }
}
