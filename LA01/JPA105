import java.util.Scanner;

public class JPA105 {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // 輸入存錢筒中各類硬幣的數量
        System.out.println("請輸入 1 元硬幣的數量：");
        int oneYuan = scanner.nextInt();
        System.out.println("請輸入 5 元硬幣的數量：");
        int fiveYuan = scanner.nextInt();
        System.out.println("請輸入 10 元硬幣的數量：");
        int tenYuan = scanner.nextInt();
        System.out.println("請輸入 50 元硬幣的數量：");
        int fiftyYuan = scanner.nextInt();

        // 計算存錢筒中的總金額
        int totalAmount = oneYuan * 1 + fiveYuan * 5 + tenYuan * 10 + fiftyYuan * 50;

        // 輸出結果
        System.out.println("存錢筒中的總金額為：" + totalAmount);
    }
}
